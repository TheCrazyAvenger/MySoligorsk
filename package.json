{
  "name": "mysoligorsk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "clean-both": "yarn clean-android && yarn clean-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "prettier-write": "prettier --write",
    "prettier-project": "yarn run prettier-write .",
    "type-check": "tsc",
    "run-android:dev": "NODE_ENV=dev envscript.sh && npx react-native run-android --variant devDebug --appIdSuffix app.dev",
    "run-android:stage": "NODE_ENV=stage envscript.sh && npx react-native run-android --variant stageDebug --appIdSuffix app.stage",
    "run-android:prod": "NODE_ENV=prod envscript.sh && npx react-native run-android --variant prodDebug --appIdSuffix app.prod",
    "start:dev": "NODE_ENV=dev yarn start --reset-cache",
    "start:stage": "NODE_ENV=stage yarn start --reset-cache",
    "start:prod": "NODE_ENV=prod yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.4",
    "@react-native-firebase/analytics": "^14.8.0",
    "@react-native-firebase/app": "^14.8.0",
    "@react-native-firebase/auth": "^14.9.3",
    "@react-native-firebase/crashlytics": "^14.8.0",
    "@react-native-firebase/dynamic-links": "^14.9.4",
    "@react-native-firebase/firestore": "^14.9.4",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/material-bottom-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "@reduxjs/toolkit": "^1.8.1",
    "formik": "^2.2.9",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^5.0.1",
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-animatable": "^1.3.3",
    "react-native-autocomplete-dropdown": "^2.0.6",
    "react-native-background-timer": "^2.4.1",
    "react-native-bouncy-checkbox": "^2.1.10",
    "react-native-bouncy-checkbox-group": "^0.1.0",
    "react-native-date-picker": "^4.2.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^2.4.1",
    "react-native-google-places-autocomplete": "^2.4.1",
    "react-native-maps": "^0.31.1",
    "react-native-paper": "^4.12.1",
    "react-native-safe-area-context": "^4.2.4",
    "react-native-screens": "^3.13.1",
    "react-native-shared-element": "^0.8.4",
    "react-native-splash-screen": "^3.3.0",
    "react-native-vector-icons": "^9.1.0",
    "react-navigation-shared-element": "^3.1.3",
    "react-redux": "^8.0.1",
    "redux-persist": "^6.0.0",
    "rn-tooltip": "^3.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.67.3",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-vector-icons": "^6.4.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-react": "^7.29.4",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "lint-staged": "^12.4.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "pull-lock": "^1.0.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "lint-staged": {
    "*.@(js|ts|tsx)": [
      "yarn eslint --fix",
      "yarn prettier-write"
    ],
    "*.@(json|md)": [
      "yarn prettier-write"
    ]
  },
  "pull-lock": {
    "yarn.lock": "yarn install"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxSingleQuote": true,
    "singleQuote": true
  },
  "resolutions": {
    "@types/react": "^17",
    "metro-inspector-proxy/ws": "5.2.3",
    "metro/ws": "5.2.3"
  }
}
